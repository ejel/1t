{
    "name": "1t",
    "version": "0.1",
    "manifest_version": 2,
    "browser_action" : {
    },
    "background": {
        "scripts": ["background.js"],
        "persistent": true
    },
    "permissions": [
        "tabs",
        "https://ajax.googleapis.com/"
    ],
    "content_security_policy": "script-src 'self' https://ajax.googleapis.com; object-src 'self'"


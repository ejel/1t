{
    "name": "TabHeaven",
    "version": "0.1",
    "manifest_version": 2,
    "browser_action" : {
    },
    "background": {
        "scripts": ["js/background.js"],
        "persistent": true
    },
    "permissions": [
        "tabs",
        "storage",
        "<all_urls>",
        "http://*/*",
        "https://*/*"
    ],
    "content_security_policy": "script-src 'self' https://ajax.googleapis.com; object-src 'self'"

}
